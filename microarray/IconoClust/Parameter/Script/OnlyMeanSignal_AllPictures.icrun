		name_match = os.path.splitext( name_match )[0] + &quot;.txt&quot;
		name_match = os.path.join( ParamGlobals.GetIconoPath(), &quot;Parameter&quot;, &quot;Layout&quot;, &quot;Substance&quot;, name_match ) 
