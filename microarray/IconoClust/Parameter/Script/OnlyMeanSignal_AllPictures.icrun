print &quot;---  Script adapted  by Ariel Vina Rodriguez !!!!!!!!!!   &quot;
print &quot;Signal changed from 1 to 0 ?? 1: = 1 - Mean / Local Background &quot;
print &quot; or to 0:  = Local Background - Mean &quot;

BRIGHT = 1     	# color
DARK = 2

MANUAL = 0		# Reference mode
AUTO = 1
